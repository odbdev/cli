CREATE TABLE IF NOT EXISTS foobars (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  text TEXT,
  created_at DATETIME DEFAULT (strftime('%Y-%m-%dT%H:%M:%fZ', 'now')),
  updated_at DATETIME DEFAULT (strftime('%Y-%m-%dT%H:%M:%fZ', 'now'))
);
